{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch SubBox UI",
            "program": "${workspaceFolder}/subbox-ui/build/main.js",
            "cwd": "${workspaceFolder}/subbox-ui",
            "outFiles": [
                "${workspaceFolder}/subbox-ui/**/*.js"
            ],
            "env": {
                "NODE_ENV": "dev"
            },
            "preLaunchTask": "tsc: build - subbox-ui/tsconfig.json",
            "console": "integratedTerminal"
        }
    ]
}